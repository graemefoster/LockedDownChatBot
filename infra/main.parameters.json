{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "dnsResourceName": {
      "value": "${DNS_RESOURCE_NAME}"
    },
    "dnsResourceGroupName": {
      "value": "${DNS_RESOURCE_RG}"
    },
    "chatApiCustomHost": {
      "value": "${CHAT_API_CUSTOM_HOST}"
    },
    "openAiResourceGroupName": {
      "value": "${OPENAI_RESOURCE_RG}"
    },
    "openAiResourceName": {
      "value": "${OPENAI_RESOURCE_NAME}"
    },
    "openAiModel": {
      "value": "${OPENAI_MODEL}"
    },
    "openAiEmbeddingModel": {
      "value": "${OPENAI_EMBEDDING_MODEL}"
    },
    "localBotAadId": {
      "value": "${LOCAL_BOT_AAD_ID}"
    },
    "localBotAadSecret": {
      "value": "${LOCAL_BOT_AAD_SECRET}"
    },
    "localBotAadTenantType": {
      "value": "${LOCAL_BOT_AAD_TenantType}"
    },
    "optionalAadTenantId": {
      "value": "${OPTIONAL_API_AAD_TENANT_ID}"
    },
    "optionalAadClientId": {
      "value": "${OPTIONAL_API_AAD_CLIENT_ID}"
    },
    "optionalAadClientSecret": {
      "value": "${OPTIONAL_API_AAD_CLIENT_SECRET}"
    },
    "optionalAadRequiredScopes": {
      "value": "${OPTIONAL_API_AAD_REQUIRED_SCOPES}"
    },
    "deployEdgeSecurity": {
      "value": "${DEPLOY_EDGE_SECURITY}"
    },
    "openAiLocation": {
      "value": "${OPENAI_LOCATION}"
    }
  }
}